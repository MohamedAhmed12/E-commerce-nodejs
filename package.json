{
  "name": "mp-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "prebuild": "rimraf dist && yarn run generate:graphql-types",
    "postbuild": "copyfiles 'src/**/*.graphql' dist",
    "build": "nest build",
    "start:dev": "run-p \"watch:*\" --",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "watch:server": "cross-env NODE_ENV=development nest start --watch",
    "watch:graphql-types": "ts-node ./scripts/generate-graphql-types.ts --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=testing jest --config ./test/jest-e2e.json",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage",
    "generate:graphql-types": "ts-node ./scripts/generate-graphql-types.ts",
    "typeorm": "cross-env NODE_ENV=development node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@casl/ability": "^5.3.1",
    "@mailchimp/mailchimp_transactional": "^1.0.26",
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/event-emitter": "^0.0.3",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/terminus": "^7.2.0",
    "@nestjs/typeorm": "^7.1.5",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.1",
    "apollo-server-express": "^2.25.0",
    "aws-sdk": "^2.948.0",
    "bcrypt": "^5.0.1",
    "bignumber.js": "^9.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.5",
    "graphql-upload": "^12.0.0",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "nestjs-pino": "^1.4.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "short-uuid": "^4.2.0",
    "typeorm": "0.2.32",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.17",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/luxon": "^1.26.5",
    "@types/node": "^15.6.1",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.2",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^5.0.1",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "testcontainers": "^7.11.1",
    "ts-jest": "^27.0.1",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}